        <section id="Edit_Student" class="Edit_Student">
            <h1 class="we2">Edit Student</h1>
            <form method='POST' action='http://localhost:3000/edit/student'>
              <!-- Roll_no, S_Fname, S_Lname, Gender, DOB, Year_enrolled, S_password, Email, Address, Dept_id -->
                <div class="form-row">
                  <div class="form-group col-md-6">
                    <label>First Name</label>
                    <input type="text" name='S_Fname' class="form-control" id="fname" placeholder="Fname" required>
                  </div>
                  <div class="form-group col-md-6">
                    <label>Last name</label>
                    <input name='S_Lname' type="text" class="form-control" id="lname" placeholder="Lname" > 
                  </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-6">
                      <label>DOB</label>
                      <input name='DOB' type="date" class="form-control" id="DOB" placeholder="DOB" required>
                    </div>
                    <div class="form-group col-md-6">
                      <label>Roll No</label>
                      <input name="Roll_no" type="text" class="form-control" id="Rollno" placeholder="Rollno" required>
                    </div>
                </div>
                <div class="form-group">
                  <label>Email</label>
                  <input name='Email' type="email" class="form-control" id="Email" placeholder="@gmail.com" required>
                </div>
                <div class="form-group">
                  <label>Address</label>
                  <input name="Address" type="text" class="form-control" id="Address" placeholder="Address" required>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label>Department</label>
                        <select name="Dept_id" id="inputState" class="form-control" required>
                        
                        <option selected>Choose...</option>
                        <option value='CSE'>CSE</option>
                        <option value='ECE'>ECE</option>
                        <option value='EEE'>EEE</option>
                        <option value='MECH'>MECH</option>
                        <option value='AEROSPACE'>AEE</option>
                        <option value='CIVIL'>CIV</option>
                        <option value='EIE'>EIE</option>
                        <option value='MAT'>MAT</option>
                        <option value='CHE'>CHE</option>
                        <option value='PHY'>PHY</option>
                    </select>
                    </div>
                    <div class="form-group col-md-6">
                      <label>Year Enrolled</label>
                      <input name="Year_enrolled" type="date" class="form-control" id="phone" required>
                    </div>
                </div>
                <div class="form-row">
                  <div class="form-group col-md-6">
                      <label>Gender</label>
                      <select name="Gender" id="inputState" class="form-control" required>
                      
                      <option selected>Choose...</option>
                      <option>M</option>
                      <option>F</option>
                  </select>
                  </div>
                  <div class="form-group col-md-6">
                    <label>Phone number</label>
                    <input type="number" class="form-control" id="phone" placeholder="Phoneno"required>
                  </div>
                  <div class="form-group col-md-6">
                    <label>Student Password:</label>
                    <input name='S_password' type="password" class="form-control" id="password" placeholder="password"required>
                  </div>
              </div>
          
            
                <button type="submit" class="btn btn-primary submit" id="submit">Submit</button>
              </form>          
              <br>

          
        </section>
