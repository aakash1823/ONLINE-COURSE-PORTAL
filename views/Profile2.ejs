  <!--================ End Header Menu Area =================-->

  <!--================Home Banner Area =================-->
  <section class="banner_area">
    <div class="banner_inner d-flex align-items-center">
      <div class="overlay"></div>
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-lg-6">
            <div class="banner_content text-center">
              <h2><input type="text" name="username" id="feedbackusername" style="background-color: black; color: blanchedalmond; text-align: center; border:black;" readonly></h2>
              <div class="page_link">
                
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!--================End Home Banner Area =================-->
<section style="background-color:#fffefe; text-align: center;">
    
    <div class="container lve" style="margin-top: 2%;">
      <table class="table table-hover table-striped aa" id="data">
        <thead class="thead-dark">
          <% faculties.forEach(function(faculty) {%>

          <tr>
            <td class="we4" scope="col">Faculty id</th>
            <td class="we4" scope="col"><%= faculty.Faculty_id  %></th>
          </tr>
          <tr>
            <td class="we4" scope="col">First Name</th>
            <td class="we4" scope="col"><%= faculty.F_Fname %></th>
          </tr>
          <tr>
            <td class="we4" scope="col">Last Name</th>
            <td class="we4" scope="col"><%= faculty.F_Lname %></th>
          </tr>
          <tr>
            <td class="we4" scope="col">Email</th>
            <td class="we4" scope="col"><%= faculty.Email %></th>
          </tr>
          <tr>
            <td class="we4" scope="col">Gender</th>
            <td class="we4" scope="col"><%= faculty.Gender %></th>
          </tr>
         
          <tr>
            <td class="we4" scope="col">Dept</th>
            <td class="we4" scope="col"><%= faculty.Dept_id %></th>
          </tr>

          <% }); %>
        </thead>


      </table>
    </div>
  
    
</section>


  <script>
 
    document.getElementById("feedbackusername").value=localStorage.getItem('username');
    document.getElementById("ln").value=localStorage.getItem('username');

  </script>

  <!-- Optional JavaScript -->
  <!-- jQuery first, then Popper.js, then Bootstrap JS -->
  <script src="extra\jquery-3.2.1.min.js"></script>
