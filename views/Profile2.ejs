  <script src="extra\owl-carousel-thumb.min.js"></script>
  <script src="extra\jquery.ajaxchimp.min.js"></script>
  <script src="extra\mail-script.js"></script>
  <!--gmaps Js-->

  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCjCGmQ0Uq4exrzdcL6rvxywDDOvfAu6eE"></script>
  <script src="extra\gmaps.min.js"></script>
  <script src="extra\theme.js"></script>
