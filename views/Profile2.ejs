<body>
  <!--================ Start Header Menu Area =================-->
  <header class="header_area">
    <div class="main_menu">

      <nav class="navbar navbar-expand-lg navbar-light">
        <div class="container">
          <!-- Brand and toggle get grouped for better mobile display -->
          <a class="navbar-brand logo_h" href="index.html"><img src="img/logo.png" alt="" /></a>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="icon-bar"></span> <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <!-- Collect the nav links, forms, and other content for toggling -->
          <div class="collapse navbar-collapse offset" id="navbarSupportedContent">
            <ul class="nav navbar-nav menu_nav ml-auto">
              
                <li class="nav-item aqwe ">
                  <form method='POST' action='http://localhost:3000/ViewCourse1 '>
                    <button type="submit" class="cldr" id="ln" name="username"><a class="nav-link"  >Courses</a></button>
                </form>                  </li>
                <li class="nav-item active">
                  <form method='POST' action='http://localhost:3000/Profile '>
                    <button type="submit" class="cldr" id="ln1" name="username"><a class="nav-link"  >Profile</a></button>
                </form>
                </li>
                <li class="nav-item">
                  <a class="nav-link" href="login.html">Log Out</a>
                </li>
            </ul>
          </div>
        </div>
      </nav>
    </div>
