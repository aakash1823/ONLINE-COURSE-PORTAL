  <link rel="icon" href="img/favicon.png" type="image/png" />
  <title>Courses Details</title>
  <!-- Bootstrap CSS -->
  <script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>

  <link rel="stylesheet" href="extra\bootstrap\css\bootstrap.css" />
  <link rel="stylesheet" href="extra\flaticon.css" />
  <link rel="stylesheet" href="extra\themify-icons.css" />
  <link rel="stylesheet" href="extra\vendors\owl-carousel\owl.carousel.min.css" />
  <link rel="stylesheet" href="extra\vendors\nice-select\css\nice-select.css" />
  <!-- main css -->
  <link rel="stylesheet" href="css\style1.css" />
  <link href="css/style.css" rel="stylesheet">

</head>

<body>
  <!--================ Start Header Menu Area =================-->
  <header class="header_area">
    <div class="main_menu">

      <nav class="navbar navbar-expand-lg navbar-light">
        <div class="container">
          <!-- Brand and toggle get grouped for better mobile display -->
          <a class="navbar-brand logo_h" href="index.html"><img src="img/logo.png" alt="" /></a>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="icon-bar"></span> <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <!-- Collect the nav links, forms, and other content for toggling -->
          <div class="collapse navbar-collapse offset" id="navbarSupportedContent">
            <ul class="nav navbar-nav menu_nav ml-auto">

              <li class="nav-item  ">
                <form method='POST' action='http://localhost:3000/ViewCourse '>
                  <button type="submit" class="cldr" id="ln" name="username"><a class="nav-link">Courses</a></button>
                </form>
              </li>
              <li class="nav-item active">
                <form method='POST' action='http://localhost:3000/Profile '>
                  <button type="submit" class="cldr" id="ln1" name="username"><a class="nav-link">Profile</a></button>
                </form>
              <li class="nav-item">
                <a class="nav-link" href="login.html">Log Out</a>
              </li>
            </ul>
          </div>
        </div>
      </nav>
    </div>
  </header>
  <!--================ End Header Menu Area =================-->

  <!--================Home Banner Area =================-->
  <section class="banner_area">
    <div class="banner_inner d-flex align-items-center">
      <div class="overlay"></div>
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-lg-6">
            <div class="banner_content text-center">
              <h2><input type="text" name="username" id="feedbackusername"
                  style="background-color: black; color: blanchedalmond; text-align: center; border:black;" readonly>
              </h2>
              <div class="page_link">

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!--================End Home Banner Area =================-->
  <section style="background-color:#f0eeee; text-align: center;">

    <div class="container lve" style="margin-top: 2%;">
      <table class="table table-hover table-striped aa " id="data">
        <thead class="thead-dark">
          <% students.forEach(function(student) {%>

          <tr>
            <td class="we4" scope="col">Roll no</th>
            <td class="we4" scope="col"><%= student.Roll_no  %></th>
          </tr>
          <tr>
            <td class="we4" scope="col">First Name</th>
            <td class="we4" scope="col"><%= student.S_Fname %></th>
          </tr>
          <tr>
            <td class="we4" scope="col">Last Name</th>
            <td class="we4" scope="col"><%= student.S_Lname %></th>
          </tr>
          <tr>
            <td class="we4" scope="col">Email</th>
            <td class="we4" scope="col"><%= student.Email %></th>
          </tr>
          <tr>
            <td class="we4" scope="col">Gender</th>
            <td class="we4" scope="col"><%= student.Gender %></th>
          </tr>
          <tr>
            <td class="we4" scope="col">Address</th>
            <td class="we4" scope="col"><%= student.Address %></th>
          </tr>
          <tr>
            <td class="we4" scope="col">Year Enrolled</th>
            <td class="we4" scope="col"><%= student.Year_enrolled.toDateString() %></th>
          </tr>
          <tr>
            <td class="we4" scope="col">Dept</th>
            <td class="we4" scope="col"><%= student.Dept_id %></th>
          </tr>
          <% }); %>
        </thead>


      </table>
    </div>
  </section>


  <script>

    document.getElementById("feedbackusername").value = localStorage.getItem('username');
    document.getElementById("ln").value = localStorage.getItem('username');
    document.getElementById("ln1").value = localStorage.getItem('username');

  </script>

